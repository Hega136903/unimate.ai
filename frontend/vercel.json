{
  "version": 2,
  "name": "unimate-ai",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@unimate-api-url",
    "NEXT_PUBLIC_APP_URL": "@unimate-app-url"
  }
}
